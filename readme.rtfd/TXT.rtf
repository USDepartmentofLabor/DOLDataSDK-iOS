\f2\fs22 \cf3 \cb4 //API  and URL constants\cf5 \
\cf3 #define\cf5  API_KEY \cf3 @\cf9 "YOUR API KEY"\cf5  \
\cf3 #define\cf5  API_SECRET \cf3 @\cf9 "YOUR SHARED SECRET"\cf5  \
